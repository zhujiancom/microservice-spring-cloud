
server:
  port: 8040
eureka:
  client:
    service-url:
      defaultZone: http://user:password@localhost:8761/eureka
    healthcheck:
      enable: true
    instance:
      prefer-ip-address: true
spring:
  application:
    name: microservice-gateway-zuul
zuul:
  ignored-services: '*'
  routes:
    microservice-provider-user: /user/**

